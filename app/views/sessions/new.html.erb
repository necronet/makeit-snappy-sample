<%= provide :title, "Make it Snapy Q&A - Login" %>

<h1> Login </h1>

<%= form_for(:session, url: sessions_path) do |f| %>
<p>
<%= f.label :username%><br />
<%= f.text_field :username%><br />
</p>

<p>
<%= f.label :password %><br />
<%= f.password_field :password%><br />
</p>

<p>
<%= f.submit "Login" %><br />
</p>

<% end %>
